{
  "author": "Steve Maze",
  "bugs": {
    "url": "https://github.com/WorldOfMaze/waterguru-api-js/issues"
  },
  "dependencies": {
    "@aws-sdk/client-cognito-identity": "^3.893.0",
    "@aws-sdk/client-lambda": "^3.893.0",
    "amazon-cognito-identity-js": "^6.3.15",
    "express": "^5.1.0",
    "node-fetch": "^3.3.2"
  },
  "description": "Unofficial TypeScript module for retrieving WaterGuru data.",
  "devDependencies": {
    "@types/express": "^5.0.3",
    "@types/jest": "^30.0.0",
    "@types/node": "^20.11.0",
    "eslint": "^8.50.0",
    "http-server": "^14.1.1",
    "jest": "^30.1.3",
    "ts-jest": "^29.4.4",
    "ts-node": "^10.9.2",
    "typescript": "^5.6.3"
  },
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./client": {
      "import": "./dist/client.mjs",
      "require": "./dist/client.cjs"
    }
  },
  "files": [
    "dist/**/*",
    "LICENSE",
    "README.md"
  ],
  "homepage": "https://github.com/WorldOfMaze/waterguru-api.ts/blob/main/README.md",
  "keywords": [
    "api",
    "typescript",
    "waterguru"
  ],
  "license": "MIT",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "name": "waterguru-api-js",
  "repository": {
    "type": "git",
    "url": "https://github.com/WorldOfMaze/waterguru-api-js"
  },
  "scripts": {
    "build": "tsup",
    "build:demo": "tsc -p tsconfig.demo.json",
    "lint": "eslint . --ext .ts",
    "prepare": "npm run build",
    "publish:public": "npm run build & npm publish --access public",
    "test": "jest"
  },
  "sideEffects": false,
  "types": "dist/index.d.ts",
  "version": "1.1.0"
}
